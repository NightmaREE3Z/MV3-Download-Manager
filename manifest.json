{
  "name": "MV3 Download Manager",
  "short_name": "MV3DLManager",
  "version": "3.0",
  "manifest_version": 3,
  "description": "A clean and modern download manager for your everyday download tasks.",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/iconblue.png",
      "48": "icons/iconblue48.png",
      "128": "icons/iconblue128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Downloads"
  },
  "icons": {
    "16": "icons/iconblue.png",
    "48": "icons/iconblue48.png",
    "128": "icons/iconblue128.png"
  },
  "permissions": [
    "downloads",
    "downloads.open",
    "downloads.shelf"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icons/icon16.png",
        "icons/icon48.png",
        "icons/icon128.png",
        "icons/icongreen.png",
        "icons/iconyellow.png"
      ],
      "matches": []
    }
  ]
}
